<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Test Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")    
</head>
<body ng-app="testModule">   
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Test Application</p>
        </footer>
    </div>

    <script src="~/Scripts/libs/angularjs/angular.js"></script>
    <script src="~/Scripts/libs/angularjs/angular-route.js"></script>
    <script src="~/Scripts/libs/angularjs/angular-messages.js"></script>
    <script src="~/Scripts/app/pages/test.module.js"></script>
    <script src="~/Scripts/libs/custom-pagination/custom-pagination.js"></script> 
    
    @Scripts.Render("~/bundles/angular")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")

    @RenderSection("FootScripts", false)
</body>

</html>
